    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "helmet": "^3.20.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.6.10",
    "node-pre-gyp": "^0.13.0",
    "short-uuid": "^3.1.1",
    "swagger-node-express": "^2.1.3",
    "swagger-ui-express": "^4.1.2"
  },
  "devDependencies": {
    "coveralls": "^3.0.9",
    "cross-env": "^7.0.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "factory-girl": "^5.0.4",
    "faker": "^4.1.0",
